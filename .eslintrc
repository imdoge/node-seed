{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "jest": true
  },
  "root": true,
  "rules": {
    "quotes": 0,
    "no-var": 0,
    "no-script-url": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "camelcase": 0,
    "prefer-const": 0,
    "prefer-destructuring": 0,
    "semi": [1, "always"],
    "object-curly-newline": [0, {
      "multiline": true,
      "consistent": true
    }],
    "constructor-super": 0,
    "operator-linebreak": 0,
    "comma-dangle": 0,
    "eol-last": 1,
    "func-names": 0,
    "guard-for-in": 0,
    "one-var": 1,
    "no-trailing-spaces": 1,
    "no-undef": 1,
    "no-unused-vars": [1, { "vars": "all", "args": "after-used" }],
    "spaced-comment": 0,
    "vars-on-top": 0,
    "id-length": 0,
    "no-magic-numbers": 0,
    "linebreak-style": [0, "windows"],
    "object-curly-spacing": [0, "never"],
    "space-in-parens": [0, "never"],
    "space-after-keywords": [0, "always"],
    "space-before-function-paren": [0, "always"],
    "import/no-unresolved": 0,
    "prefer-arrow-callback": 0,
    "no-underscore-dangle": 0,
    "prefer-template": 0,
    "quote-props": 0,
    "consistent-return": 0,
    "arrow-parens": 0,
    "import/extensions": 0,
    "no-restricted-syntax": 0,
    "no-prototype-builtins": 0,
    "no-unused-expressions": 0,
    "no-use-before-define": 0,
    "no-plusplus": 0,
    "no-await-in-loop": 0,
    "no-else-return": 0
  }
}
